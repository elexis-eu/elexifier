{
  "name": "elexifier",
  "version": "1.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "deploy:prod": "ng build --prod && docker-compose -f docker-compose.production.yml -p elexifier up -d --build",
    "deploy:staging": "ng build --configuration=staging && docker-compose -f docker-compose.staging.yml -p elexifier up -d --build",
    "deploy:dev": "ng build --prod && docker-compose -p elexifier build && docker-compose -p elexifier up -d",
    "e2e": "ng e2e",
    "inject-revision": "node ./utils/inject-revision",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "prebuild:staging": "npm run inject-revision",
    "start": "ng serve",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.3",
    "@angular/cdk": "8.1.0",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@babel/compat-data": "^7.8.0",
    "@ng-bootstrap/ng-bootstrap": "5.1.4",
    "@sentry/browser": "5.6.3",
    "angular-feather": "6.0.2",
    "bootstrap": "4.3.1",
    "file-saver": "2.0.2",
    "ngx-custom-validators": "8.0.0",
    "primeicons": "2.0.0",
    "primeng": "8.0.1",
    "prismjs": "1.17.1",
    "rxjs": "~6.4.0",
    "ts-optchain": "0.1.8",
    "tslib": "1.9.0",
    "vkbeautify": "0.99.3",
    "xsm": "1.6.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@sentry/cli": "^1.52.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/prismjs": "1.16.0",
    "codelyzer": "5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "~3.4.3"
  }
}
