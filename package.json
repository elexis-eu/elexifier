{
  "name": "elexifier",
  "version": "1.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "ng build --prod",
    "deploy:prod": "ng build --prod && docker-compose -f docker-compose.production.yml -p elexifier up -d --build",
    "deploy:staging": "ng build --configuration=staging && docker-compose -f docker-compose.staging.yml -p elexifier up -d --build",
    "deploy:dev": "ng build --prod && docker-compose -p elexifier build && docker-compose -p elexifier up -d",
    "e2e": "ng e2e",
    "inject-revision": "node ./utils/inject-revision",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "ng": "ng",
    "prebuild:staging": "npm run inject-revision",
    "start": "ng serve",
    "test": "ng test",
    "preinstall": "npx npm-force-resolutions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.1",
    "@angular/cdk": "8.1.0",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/localize": "^9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@babel/compat-data": "^7.9.0",
    "@fullcalendar/core": "^4.4.0",
    "@ng-bootstrap/ng-bootstrap": "5.1.4",
    "@sentry/browser": "5.6.3",
    "angular-feather": "6.0.2",
    "bootstrap": "4.3.1",
    "chart.js": "^2.9.3",
    "file-saver": "2.0.2",
    "ngx-custom-validators": "8.0.0",
    "primeicons": "2.0.0",
    "primeng": "^9.0.5",
    "prismjs": "1.17.1",
    "quill": "^1.3.7",
    "rxjs": "~6.5.5",
    "ts-optchain": "0.1.8",
    "tslib": "^1.10.0",
    "vkbeautify": "0.99.3",
    "xsm": "1.6.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@sentry/cli": "^1.52.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/prismjs": "1.16.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.5",
    "protractor": "^5.4.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "~3.8.3"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "acorn": "^6.4.1",
    "kind-of": "^6.0.3"
  }
}
